{
    "printWidth": 160,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": false,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "insertPragma": true,
    "overrides": [
        {
            "files": ".editorconfig",
            "options": {
                "parser": "yaml"
            }
        },
        {
            "files": "LICENSE",
            "options": {
                "parser": "markdown"
            }
        }
    ]
}